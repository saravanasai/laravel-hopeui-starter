{
    "name": "saravanasai/laravel-hopeui",
    "description": "Laravel Hope -UI based starter kit",
    "keywords": [
        "Hope UI",
        "laravel",
        "Starter Kit"
    ],
    "homepage": "https://github.com/saravanasai/laravel-hopeui-starter",
    "license": "MIT",
    "authors": [
        {
            "name": "saravanasai",
            "email": "saravanasai002@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0|^10.0",
        "livewire/livewire": "^2.10",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.6.0",
        "nunomaduro/collision": "^6.4.0",
        "nunomaduro/larastan": "^2.4.1",
        "orchestra/testbench": "^7.22",
        "pestphp/pest": "^1.22.4",
        "pestphp/pest-plugin-laravel": "^1.4.0",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9.6",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Saravanasai\\Hopeui\\": "src",
            "Saravanasai\\Hopeui\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saravanasai\\Hopeui\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saravanasai\\Hopeui\\HopeUiServiceProvider"
            ]
        }
    }
}
